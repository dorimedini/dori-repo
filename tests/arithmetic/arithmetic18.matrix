	matrix m3(3,2) = [1, 2; 4, 5; 7, 8];
	matrix m4(3,1) = [3; 6; 9];
	matrix m33(3,3);

	m33=m1 - m2 + m1;
	print m33;
	m33=m1 + m2 - m1;
	print m33;

	m33=m1 - m1*m2;
	print m33;
	m33=m1 + m1*m2;
	print m33;

	m33=m1 + -m2;
	print m33;
	m33=m1 + --m2;
	print m33;
	m33=m1 + ---m2;
	print m33;
	m33=m1 * -m2;
	print m33;
	m33=m1 * --m2;
	print m33;
	m33=m1 * ---m2;
		
