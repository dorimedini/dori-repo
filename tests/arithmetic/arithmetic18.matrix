{
	int a = 2;
	int b = 5;
	int x;
	
	// Addition and subtraction have same precedence so the earlier should operate first
	x=a - b + a;
	print x;
	x=a + b - a;
	print x;

	// Multiplication and division go before addition and subtraction
	x=a - a*b;
	print x;
	x=b + b/b;
	print x;
	x=a * b / a;
	print x;
	x=a / a * b;
	print x;

	// Unary minus comes before arithmetic operations
	x=a + -b;
	print x;
	x=a + --b;
	print x;
	x=a + ---b;
	print x;
	x=b / -a;
	print x;
	x=b / --a;
	print x;
	x=b / ---a;
	print x;


